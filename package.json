{
  "name": "nuxt-tdd",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "preinstall": "npx npm-force-resolutions",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:fix": "eslint --max-warnings 0 '**/*.{js,vue}' --ignore-path .eslintignore .",
    "format": "prettier --check '**/*.{js,vue}'",
    "format:fix": "prettier --write '**/*.{js,vue}'",
    "test": "nyc mochapack --webpack-config tests/webpack.config.js --require tests/setup.js tests/**/*.spec.js",
    "test-no-cov": "mochapack --webpack-config tests/webpack.config.js --require tests/setup.js tests/**/*.spec.js"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxtjs/auth-next": "5.0.0-1637745161.ea53f98",
    "@nuxtjs/axios": "^5.13.6",
    "core-js": "^3.15.1",
    "fs-extra": "^9.1.0",
    "hls.js": "1.1.5",
    "nuxt": "^2.13.3",
    "prettier": "^2.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@vue/compiler-dom": "^3.2.40",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.7.0",
    "expect": "^29.0.3",
    "husky": "^4.3.8",
    "jsdom": "20.0.0",
    "jsdom-global": "3.0.2",
    "lint-staged": "^10.5.4",
    "mocha": "^10.0.0",
    "mocha-junit-reporter": "^2.0.2",
    "mochapack": "^2.1.4",
    "nuxt-compress": "^5.0.0",
    "nyc": "^15.1.0",
    "prettier": "2.6.2",
    "sass": "^1.51.0",
    "sass-loader": "^10.2.1",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^21.0.0",
    "vue-jest": "^3.0.5",
    "vue-test-utils": "^1.0.0-beta.11"
  },
  "resolutions": {
    "glob-parent": "5.1.2",
    "nth-check": "2.0.1",
    "parse-path": "5.0.0",
    "parse-url": "8.1.0"
  }
}
